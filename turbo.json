{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "build-dev": {
      "dependsOn": ["^build-dev"]
    },
    "package": {
      "cache": false,
      "dependsOn": ["build"]
    },
    "clean": {
      "cache": false,
      "outputs": []
    },
    "coverage": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "demo": {
      "cache": false,
      "dependsOn": ["build-dev"],
      "outputs": []
    }
  }
}
