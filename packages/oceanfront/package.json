{
  "name": "oceanfront",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/oceanfront.umd.js",
  "module": "dist/oceanfront.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/oceanfront.mjs",
      "require": "./dist/oceanfront.umd.js"
    },
    "./css": "./dist/oceanfront.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "build-dev": "vite build -m development",
    "package": "yarn pack",
    "clean": "rm -rf .turbo .vite node_modules dist",
    "coverage": "vitest run --coverage",
    "demo": "vite build -m development --watch",
    "lint": "eslint src --ext .ts,.vue",
    "test": "vitest --watch=false"
  },
  "peerDependencies": {
    "vue": "~3.5.13"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/resize-observer-browser": "~0.1.11",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitejs/plugin-vue": "~5.2.1",
    "@vitest/coverage-v8": "^3.1.1",
    "@vue/test-utils": "2.4.6",
    "dependency-cruiser": "^16.10.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vitest": "0.3.25",
    "eslint-plugin-vue": "^9.22.0",
    "jsdom": "~25.0.1",
    "prettier": "~3.2.5",
    "tslib": "~2.6.2",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "~0.17.5",
    "vite": "~6.2.5",
    "vite-plugin-dts": "~4.5.3",
    "vitest": "^3.1.1",
    "vue": "~3.5.13"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ]
}