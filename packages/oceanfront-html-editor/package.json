{
  "name": "oceanfront-html-editor",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "main": "dist/oceanfront-html-editor.umd.js",
  "module": "dist/oceanfront-html-editor.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/oceanfront-html-editor.mjs",
      "require": "./dist/oceanfront-html-editor.umd.js"
    },
    "./css": "./dist/oceanfront-html-editor.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "build-dev": "vite build -m development",
    "clean": "rm -rf .turbo .vite node_modules dist",
    "coverage": "vitest run --coverage",
    "demo": "vite build -m development --watch",
    "lint": "eslint src",
    "test": "vitest --watch=false"
  },
  "peerDependencies": {
    "oceanfront": "*"
  },
  "dependencies": {
    "@tiptap/extension-color": "=2.0.3",
    "@tiptap/extension-font-family": "=2.0.3",
    "@tiptap/extension-highlight": "=2.0.3",
    "@tiptap/extension-image": "=2.0.3",
    "@tiptap/extension-link": "=2.0.3",
    "@tiptap/extension-table": "=2.0.3",
    "@tiptap/extension-table-cell": "=2.0.3",
    "@tiptap/extension-table-header": "=2.0.3",
    "@tiptap/extension-table-row": "=2.0.3",
    "@tiptap/extension-text-align": "=2.0.3",
    "@tiptap/extension-text-style": "=2.0.3",
    "@tiptap/extension-underline": "=2.0.3",
    "@tiptap/core": "=2.0.3",
    "@tiptap/pm": "2.0.3",
    "@tiptap/starter-kit": "=2.0.3",
    "@tiptap/vue-3": "=2.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/test-utils": "2.3.2",
    "@vitest/coverage-c8": "~0.29",
    "eslint": "^8",
    "eslint-config-prettier": "^8",
    "eslint-plugin-prettier": "^4",
    "eslint-plugin-vitest": "0.0.54",
    "eslint-plugin-vue": "^9",
    "jsdom": "~21.1",
    "oceanfront": "*",
    "prettier": "~2.8",
    "tslib": "^2.4",
    "typescript": "~4.4",
    "unplugin-auto-import": "~0.15",
    "vite": "~4.3.9",
    "vitest": "^0.31.4",
    "vite-plugin-dts": "~2.0",
    "vue": "~3.2"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ]
}
