{
  "private": true,
  "version": "0.0.1",
  "name": "oceanfront-repo",
  "packageManager": "yarn@1.22.17",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --no-daemon",
    "build-dev": "turbo run build-dev --no-daemon",
    "package": "turbo run package",
    "clean": "turbo run clean && rm -rf node_modules",
    "coverage": "turbo run coverage --no-daemon",
    "demo": "turbo run demo --no-daemon",
    "lint": "turbo run lint --no-daemon",
    "test": "turbo run test --no-daemon"
  },
  "devDependencies": {
    "turbo": "^2.4.4"
  },
  "resolutions": {
    "vue": "~3.5.13",
    "vitest": "^3.2.4",
    "prosemirror-model": "1.25.0",
    "cross-spawn": "^7.0.6",
    "linkifyjs": "^4.3.2",
    "micromatch": "^4.0.8",
    "js-yaml": "^4.1.1",
    "brace-expansion": "^2.0.2",
    "js-beautify/glob": "^10.5.0",
    "test-exclude/glob": "^10.5.0",
    "flat-cache": "^4.0.1"
  }
}
